   "source": [
    "n = int(input())\n",
    "m = int(input())\n",
    "\n",
    "\n",
    "def min_breaks(n, m):\n",
    "    if n == 1 and m == 1:\n",
    "        return 0\n",
    "    elif n == 1:\n",
    "        return m - 1\n",
    "    elif m == 1:\n",
    "        return n - 1\n",
    "    else:\n",
    "        pieces = n*m\n",
    "        return pieces - 1\n",
    "\n",
    "\n",
    "print(min_breaks(n, m))  \n"
   ]
